<%
locals.OLSKPageTitle = OLSKLocalized('JBXVitrineTitle');
locals.OLSKPageDescription = OLSKLocalized('JBXVitrineDescription');
locals.OLSKPageManifest = OLSKCanonical('JBXManifestRoute');
locals.OLSKPageAssets = [
	'/_shared/__external/normalize.css/normalize.css',
	'/_shared/__external/OLSKDecor/ui-style.css',
	'/_shared/common/ui-style.css',
	'ui-style.css',
];
locals.OLSKPageUseLivereload = process.env.NODE_ENV === 'development';
locals.OLSKPagePublicConstants = {
	OLSKSharedPageCurrentLanguage: OLSKSharedPageCurrentLanguage,
};
%>

<div class="JBXVitrine OLSKDecor OLSKDecorCapped">	

<div class="JBXVitrineToolbar OLSKToolbar OLSKToolbarJustify OLSKStickyHeader OLSKCommonEdgeBottom">
	<div class="JBXVitrineCrown OLSKCommonCard OLSKCommonCrownCardMini">
		<img class="JBXVitrineCrownIcon" src="/_shared/JBXRootLink/ui-assets/identity.svg" role="presentation" />
		<section>
			<span class="JBXVitrineCrownName"><%= OLSKLocalized('JBXVitrineTitle') %></span>
		</section>
	</div>

	<%- include('../../node_modules/OLSKLanguageSwitcher/main.ejs') %>
</div>

<%- include('../../node_modules/OLSKLanding/main.ejs', {
	OLSKLandingHeadingText: OLSKLocalized('JBXVitrineDescription'),
	OLSKLandingBlurbText: OLSKLocalized('OLSKLandingBlurbText'),
	OLSKLandingActionText: OLSKLocalized('OLSKWordingOpenApp'),
	OLSKLandingActionHref: OLSKCanonical('JBXPlayRoute'),
}) %>

<h1 class="JBXVitrineFeaturesHeading"><%= OLSKLocalized('OLSKWordingFeatures') %></h1>

<%- include('../../node_modules/OLSKAppFeatureList/main.ejs', {
	OLSKAppFeatureListRepoURL: process.env.OLSK_REPO_URL,
}).replace(/node_modules/g, '_shared/__external') %>

<a class="JBXVitrineGuideButton OLSKDecorPress OLSKDecorPressCall" href="<%= OLSKCanonical('JBXGuideRoute') %>"><%= OLSKLocalized('OLSKWordingOpenGuide') %></a>

<h1 class="JBXVitrineGazetteHeading"><%= OLSKLocalized('OLSKGazetteHeadingText') %></h1>
<%- include('../../node_modules/OLSKGazette/main.ejs', {
	OLSKGazetteProject: 'RP_X',
}) %>

<h1 class="JBXVitrineSupportHeading"><%= OLSKLocalized('OLSKWordingFeedbackHeading') %></h1>
<p class="JBXVitrineSupportBlurb"><%= OLSKLocalized('OLSKWordingFeedbackBlurb') %></p>

<hr role="presentation" />

<%- include('../../node_modules/SWARLink/main.ejs') %>

<%- include('../../node_modules/ROCORootLink/main.ejs') %>

</div>
